        <mxCell id="N6R9nf9umuP-RXy4cG7P-18" value="http {&lt;br style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &lt;font color=&quot;#910049&quot; style=&quot;font-size: 12px;&quot;&gt;upstream mysvr&lt;/font&gt; {&amp;nbsp;&amp;nbsp;&lt;b style=&quot;border-color: var(--border-color);&quot;&gt;# 配置nginx后的服务器集群&lt;/b&gt;&lt;br style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; server 127.0.0.1:7878;&lt;br style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; server 192.168.10.121:3333 backup; &amp;nbsp; &amp;nbsp;#热备&lt;br style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; }&lt;br style=&quot;font-size: 12px;&quot;&gt;}" style="whiteSpace=wrap;fontFamily=Courier New;align=left;fontSize=12;fontStyle=0;html=1;fillColor=#fff2cc;strokeColor=#d6b656;glass=1;rounded=0;shadow=0;" parent="1" vertex="1">
